<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HappyBirthdayMuskan️" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/orange_500</item>
        <item name="colorPrimaryVariant">#c64e42</item>
        <item name="colorOnPrimary">#ffaf9b</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_light_700</item>
        <item name="colorSecondaryVariant">@color/blue_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <item name="textInputStyle">@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.HappyBirthdayMuskan️.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.HappyBirthdayMuskan️.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.HappyBirthdayMuskan️.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>